{
  "name": "website",
  "version": "0.0.1",
  "description": "Drash Land",
  "scripts": {
    "compile:drash-v1.x-api-reference": "console/compile_drash_v1.x_api_reference",
    "compile:vue-routes": "node console/compile_vue_routes.js",
    "docker:build-staging": "docker build --build-arg NODE_ENV=staging -t drashland-website .",
    "docker:build-production": "docker build --build-arg NODE_ENV=production -t drashland-website .",
    "remove-ds-store": "find . -name '.DS_Store' -type f -delete",
    "server": "deno run -A console/server.ts",
    "webpack:development": "webpack --config ./webpack.config.js --env.environment=development --hide-modules",
    "webpack:production": "webpack --config ./webpack.config.js --env.environment=production --hide-modules",
    "webpack:staging": "webpack --config ./webpack.config.js --env.environment=staging --hide-modules"
  },
  "author": "Drash Land",
  "devDependencies": {
    "@babel/core": "7.x",
    "axios": "0.x",
    "babel-loader": "8.x",
    "css-loader": "3.x",
    "markdown-it": "11.x",
    "node-sass": "4.x",
    "prismjs": "1.x",
    "pug": "3.x",
    "pug-plain-loader": "1.x",
    "sass-loader": "9.x",
    "scss-loader": "0.x",
    "style-loader": "1.x",
    "vue": "2.x",
    "vue-loader": "15.x",
    "vue-router": "3.x",
    "vue-style-loader": "4.x",
    "vue-template-compiler": "2.x",
    "webpack": "4.x",
    "webpack-cli": "3.x"
  }
}
